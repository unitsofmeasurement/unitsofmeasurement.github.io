                                            <td class="versions">                        0.8-RC3            </td>
                                            <td class="fixVersions">    &nbsp;
</td>
                                            <td class="components">                        API            </td>
                                            <td class="duedate">    &nbsp;
</td>
                                            <td class="votes">0</td>
                                            <td class="watches">2</td>
                                            <td class="thumbnail"></td>
